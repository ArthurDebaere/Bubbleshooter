<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="147.0" prefWidth="264.0" stylesheets="@../GUI/res/Style.css" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.GUI.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane maxWidth="350.0" prefHeight="147.0" prefWidth="264.0" stylesheets="@../GUI/res/Style.css">
            <children>
            <ImageView fitHeight="150.0" fitWidth="264.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@res/Screens/QuitMenu.png" />
               </image>
            </ImageView>
            <Button fx:id="btnNO" layoutX="67.0" layoutY="95.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="53.0" styleClass="makeTransparant">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="btnYES" layoutX="144.0" layoutY="95.0" mnemonicParsing="false" onAction="#btnReset_click" prefHeight="32.0" prefWidth="53.0" styleClass="makeTransparant">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            </children>
            <effect>
                <Blend />
            </effect>
        </Pane>
    </children>
</GridPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="147.0" prefWidth="327.0" stylesheets="@../GUI/res/Style.css" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.GUI.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane maxWidth="350.0" prefHeight="150.0" prefWidth="327.0" stylesheets="@../GUI/res/Style.css">
            <children>
            <ImageView fitHeight="150.0" fitWidth="327.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../GUI/res/Screens/Paused.png" />
               </image>
            </ImageView>
            <Button fx:id="btnPlay" layoutX="50.0" layoutY="61.0" mnemonicParsing="false" prefHeight="61.0" prefWidth="59.0" styleClass="makeTransparant">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="btnReplay" layoutX="134.0" layoutY="61.0" mnemonicParsing="false" onAction="#btnReset_click" prefHeight="61.0" prefWidth="59.0" styleClass="makeTransparant">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="btnMainMenu" layoutX="224.0" layoutY="61.0" mnemonicParsing="false" onAction="#backToMainfromDialog" prefHeight="61.0" prefWidth="59.0" styleClass="makeTransparant">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            </children>
            <effect>
                <Blend />
            </effect>
        </Pane>
    </children>
</GridPane>
